import CocktailsList from \"./CocktailsList\";\nimport SingleCocktail from \"./SingleCocktail\";\nimport IngredientInfo from \"./IngredientInfo\";\n\nconst Home = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  useEffect(() => {\n    const apiId = history.location.search.split(\"=\")[1];\n    if (history.location.search) {\n      dispatch(searchSingleCocktail(apiId));\n      history.replace(\"/\"); // https://stackoverflow.com/questions/61477583/remove-a-query-string-using-react-router-without-reloading-page\n    } else {\n      dispatch(searchRandomCocktail());\n    }\n  }, [dispatch]);\n\n\n  return (\n    <>\n      <CocktailsList />\n      <SingleCocktail />\n      <IngredientInfo />\n    </>\n  );\n};\n\nexport default Home;\n",["205","206"],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/reducers/index.js",[],["207","208"],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/Header.js",["209"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nimport Search from \"./Search\";\nimport SearchWithAutocomplete from \"./SearchWithAutocomplete\";\nimport Navigation from \"./Navigation\";\n\nconst Header = () => {\n\n  return (\n    <div className=\"header\">\n      <div className=\"brand\"><Link to=\"/\" className=\"brand__link\">Brand</Link></div>\n      {/* <Search /> */}\n      <SearchWithAutocomplete />\n      <Navigation />\n    </div>\n  );\n};\n\nexport default Header;\n",["210","211"],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/actions/types.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/actions/index.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/renderProps/User.js",[],["212","213"],"/Users/oksanastepanchuk/Documents/Programming/learn