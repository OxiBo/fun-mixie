          Continue with Facebook\n                </a>\n                <a\n                  href=\"\"\n                  className=\"btn btn-big btn-social signin-buttons__social btn-instagram\"\n                >\n                  {\" \"}\n                  <svg className=\"icon icon-instagram\">\n                    <use href={sprite + \"#icon-instagram-with-circle\"}></use>\n                  </svg>\n                  Continue with Instagram\n                </a>\n                <a\n                  href=\"/auth/twitter\"\n                  className=\"btn btn-big btn-social signin-buttons__social btn-twitter\"\n                >\n                  <svg className=\"icon icon-twitter\">\n                    <use href={sprite + \"#icon-twitter1\"}></use>\n                  </svg>\n                  Continue with Twitter\n                </a>\n                <Link to=\"/\" className=\"btn btn-big btn-danger\">\n                  Cancel\n                </Link>\n              </div>\n            </div>\n          </div>\n        );\n      }}\n    </User>\n  );\n};\n\nexport default SignIn;\n","/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/Test.js",[],["173","174"],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/Search.js",[],["175","176"],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/apis/cocktailsAPI.js",[],["177","178"],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/App.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/reducers/searchCocktails.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/reducers/auth.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/utils/parseCocktailRecipe.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/customHooks/useOutsideClick.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/CocktailsList.js",["179"],"import React, { useState, useEffect } from \"react\";\n\nimport Spinner from \"./Spinner\";\nimport useFetchData from \"./customHooks/useFetchData\";\nimport {\n  selectCocktailsList,\n  selectCocktailsListError,\n} from \"./selectors/cocktails\";\nimport CocktailItem from \"./CocktailItem\";\nimport Pagination from \"./Pagination\";\nimport ButtonClear from \"./ButtonClear\";\nimport { clearCocktailsList } from \"../actions\";\nimport { perPage } from \"../utils/variables\";\n\nconst CocktailsList = () => {\n  const [page, setPage] = useState(1);\n  // const [cocktailsList, setCocktailsList] = useState(null);\n\n  const { data, error, loading } = useFetchData(\n    selectCocktailsList,\n    selectCocktailsListError\n  );\n\n  useEffect(() => {\n    if (data) {\n      setPage(1);\n    }\n  }, [data]);\n\n  const indexOfLastCocktail = page * perPage;\n  const indexOfFirstCocktail = indexOfLastCocktail - perPage;\n  const currentCocktails =\n    data && data.slice(indexOfFirstCocktail, indexOfLastCocktail);\n\n  const onPageChange = (currentPage) => {\n    setPage(currentPage);\n\n    // await setCocktailsList(paginatedCocktails);\n  };\n\n  const renderCocktailList = () => {\n    const cocktails = currentCocktails.map((cocktail) => {\n      return (\n        <CocktailItem\n          key={`${cocktail.id}-${cocktail.name}`}\n          cocktail={cocktail}\n        />\n      );\n    });\n    return cocktails;\n  };\n\n  return (\n    <>\n      <ul className=\"cocktails__list\">\n        {loading ? (\n          <Spinner />\n        ) : error ? (\n          <div className=\"cocktails__list-error error\">\n            <p classList=\"error-message\">{error}</p>\n          </div>\n        ) : data.length