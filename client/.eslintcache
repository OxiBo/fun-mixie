n\n  const { data, error, loading } = useFetchData(\n    selectFaveCocktails,\n    selectFaveCocktailsError\n  );\n\n  const onPageChange = (currentPage) => {\n    setPage(currentPage);\n  };\n\n  const renderFaveCocktails = () => {\n    return (\n      <>\n        {data.total === 0 ? (\n          <h2 className=\"fave-cocktails__empty heading-tertiary\">\n            You do not have favorite cocktails yet\n          </h2>\n        ) : (\n          <ul className=\"fave-cocktails-list\">\n            {data.paginated.map((cocktail) => (\n              <FaveCocktailItem key={cocktail._id} cocktail={cocktail} />\n            ))}\n          </ul>\n        )}\n      </>\n    );\n  };\n\n  return (\n    <>\n      <div className=\"fave-cocktails\">\n        {loading ? (\n          <Spinner />\n        ) : error ? (\n          <div className=\"fave-cocktails__error\">\n            <ErrorMessage error={error} />\n          </div>\n        ) : (\n          data && renderFaveCocktails(data)\n        )}\n        {data && data.total > perPageFaveCocktails && (\n          <Pagination\n            perPage={perPageFaveCocktails}\n            total={data && data.total} //data.total\n            onPageChange={onPageChange}\n          />\n        )}\n      </div>\n    </>\n  );\n};\n\nexport default FavoriteCocktails;\n","/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/FaveCocktailItem.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/ErrorMessage.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/NotFoundPage.js",[],"/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/EmailForm.js",["258","259","260","261"],"import React, { useState, useEffect, useRef } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { emailCocktail } fr