nPageChange = (currentPage) => {\n    setPage(currentPage);\n\n    // await setCocktailsList(paginatedCocktails);\n  };\n\n  const renderCocktailsList = () => {\n    console.log(currentCocktails)\n    const cocktails = currentCocktails.map((cocktail) => {\n    \n      return (\n        <CocktailItem\n          key={`${cocktail.apiId}-${cocktail.name}`}\n          cocktail={cocktail}\n        />\n      );\n    });\n    return cocktails;\n  };\n\n  return (\n    <>\n      <ul className=\"cocktails__list\">\n        {loading ? (\n          <Spinner />\n        ) : error ? (\n          <div className=\"cocktails__list-error error\">\n            <p classList=\"error-message\">{error}</p>\n          </div>\n        ) : data && data.length > 0 ? (\n          <>\n            {renderCocktailsList()}\n            {data.length > perPage && (\n              <div className=\"cocktails__list-pagination\">\n                <Pagination\n                  perPage={perPage}\n                  total={data.length}\n                  onPageChange={onPageChange}\n                />\n              </div>\n            )}\n\n            <ButtonClear action={clearCocktailsList} />\n          </>\n        ) : (\n          false\n        )}\n      </ul>\n    </>\n  );\n};\n{\n  /* <div className=\"cocktails__list-empty no-content\">\n          <p className=\"no-content-message\">no cocktails yet</p>\n        </div> */\n}\nexport default CocktailsList;\n","/Users/oksanastepanchuk/Documents/Programming/learn_JavaScript/Fullstack/fun-mixie/client/src/components/SingleCocktail.js",["247","248"],"import React, { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\n// svg icons\nimport sprite from \"../styles/img/sprite.svg\";\nimport Spinner from \"./Spinner\";\nimport EmailForm from \"./EmailForm\";\nimport Liked from \"./Liked\";\nimport { searchIngredient } from \"../actions\";\nimport {\n  selectSingleCocktail,\n  selectSingleCocktailError,\n} from \"./selec